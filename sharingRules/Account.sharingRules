<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>CA_Meetings_Role_Access_to_Prospect</fullName>
        <accessLevel>Edit</accessLevel>
        <accountSettings>
            <caseAccessLevel>Read</caseAccessLevel>
            <contactAccessLevel>Read</contactAccessLevel>
            <opportunityAccessLevel>Read</opportunityAccessLevel>
        </accountSettings>
        <description>Gives edit access to any Prospect Account type to those with CA Meetings Role</description>
        <label>CA Meetings Role Access to Prospect</label>
        <sharedTo>
            <roleAndSubordinatesInternal>CA_Meetings_Events_Mgr</roleAndSubordinatesInternal>
        </sharedTo>
        <criteriaItems>
            <field>Type</field>
            <operation>equals</operation>
            <value>Prospect Account,&quot;Prospect, Lost Client&quot;,Avenir Prospect</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>Clinical_Accounts</fullName>
        <accessLevel>Edit</accessLevel>
