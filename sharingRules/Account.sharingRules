            <opportunityAccessLevel>Read</opportunityAccessLevel>
        </accountSettings>
        <description>Clinic Trails Roles can Modify Clinical Accounts &amp; Contacts</description>
        <label>Clinical Accounts</label>
        <sharedTo>
            <role>Clinical_Trials</role>
        </sharedTo>
        <criteriaItems>
            <field>Type</field>
            <operation>equals</operation>
            <value>Clinical Client,Clinical Prospect</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
