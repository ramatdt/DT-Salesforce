    <allowInChatterGroups>true</allowInChatterGroups>
    <compactLayoutAssignment>Custom_Survey_Question_Compact_Layout</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Custom Survey_Question__c object that allows more customization than the native Salesforce Surveys solution. The Survey Question object represents an instance of a question that is presented to a survey respondent as part of a Survey. Every Survey Question record relates to a Survey record.</description>
    <enableActivities>true</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>Private</externalSharingModel>
    <label>Survey Question</label>
    <nameField>
        <label>Survey Question Name</label>
        <trackHistory>false</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>Survey Questions</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <visibility>Public</visibility>
    <compactLayouts>
        <fullName>Custom_Survey_Question_Compact_Layout</fullName>
        <fields>Name</fields>
        <label>Custom Survey Question Compact Layout</label>
    </compactLayouts>
    <fields>
        <fullName>Survey__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>Looks up to the Survey this Survey Question relates to.</description>
        <label>Survey</label>
        <referenceTo>Survey__c</referenceTo>
        <relationshipLabel>Survey Questions</relationshipLabel>
        <relationshipName>Survey_Questions</relationshipName>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>IsLegacy__c</fullName>
        <description>Used in survey response record triggered flow. If the survey question legacy Id is blank, then IsLegacy = true.</description>
        <formula>ISBLANK(Legacy_SurveyQuestion_Id__c)</formula>
        <label>IsLegacy</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Question_Type__c</fullName>
        <description>The data type of the survey question.</description>
        <label>Question Type</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>true</sorted>
                <value>
                    <fullName>Boolean</fullName>
                    <default>false</default>
                    <label>Boolean</label>
                </value>
                <value>
                    <fullName>FreeText</fullName>
                    <default>false</default>
                    <label>FreeText</label>
                </value>
                <value>
                    <fullName>NPS</fullName>
                    <default>false</default>
                    <label>NPS</label>
                </value>
                <value>
                    <fullName>Number</fullName>
                    <default>false</default>
                    <label>Number</label>
                </value>
                <value>
                    <fullName>Picklist</fullName>
                    <default>false</default>
                    <label>Picklist</label>
                </value>
                <value>
                    <fullName>RadioButton</fullName>
                    <default>false</default>
                    <label>RadioButton</label>
                </value>
                <value>
                    <fullName>Rating</fullName>
                    <default>false</default>
                    <label>Rating</label>
                </value>
                <value>
                    <fullName>ShortText</fullName>
                    <default>false</default>
                    <label>ShortText</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Full_Question_Text__c</fullName>
        <description>The name of the Survey Question record is truncated at 255 characters. This field stores the full question text, up to 32,768 characters. This is the text that should be displayed to the end survey respondent when taking a survey.</description>
        <label>Full Question Text</label>
        <length>32768</length>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>French_Question_Text__c</fullName>
        <description>Full Question Text - French Translation</description>
        <label>French Question Text</label>
        <length>32768</length>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Legacy_SurveyQuestion_Id__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>Before the custom Survey__c object was built, Salesforce Surveys were in use. This field stores the legacy  Id for the corresponding, historical Salesforce Surveys SurveyQuestion record.</description>
        <externalId>true</externalId>
        <label>Legacy SurveyQuestion Id</label>
        <length>18</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Legacy_Survey_Version_Id__c</fullName>
        <description>Before the custom Survey__c object was built, Salesforce Surveys were in use. This field stores the legacy Id for the corresponding, historical Salesforce Survey Version record.</description>
        <externalId>false</externalId>
        <label>Legacy Survey Version Id</label>
        <length>18</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
</CustomObject>